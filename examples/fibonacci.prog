def fibonacci = func(num) do
	def a = 0
	def b = 1
	def c

	if num == 0 then
		return a
	end

	def i = 2

	while i <= num do
		c = a + b
		a = b
		b = c

		i = i + 1
	end

	return b
end

def main = func(num) do
	def number = (num) -> fibonacci
	(num, " fibonacci number is ", number) -> print

	return number
end

return (25) -> main