def obj_a = {
	hello = "world!"
}

def obj_b = {
	hello = "world!"
}

def obj_alias_a = obj_a
def obj_alias_b = obj_b

assert(obj_a == obj_a)
assert(obj_b == obj_b)

assert(obj_a != obj_b)
assert(obj_alias_a != obj_alias_b)

assert(obj_alias_a == obj_alias_a)
assert(obj_alias_a == obj_a)
assert(obj_alias_a != obj_b)

assert(obj_alias_b == obj_alias_b)
assert(obj_alias_b == obj_b)
assert(obj_alias_b != obj_a)